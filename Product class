import java.util.Scanner;

public class Product
{
    int pcode;
    String pname;
    int price;
   
    
     void input() {
        Scanner in = new Scanner(System.in);
        System.out.print("Enter product code");
        pcode= in.nextLine();
        System.out.print("Enter product name: ");
        pname = in.nextLine();
        System.out.print("Enter price: ");
        price= in.nextLine();
    }
    
    public void display() {
        System.out.println("product code: " + pcode);
        System.out.println("Enter product name: " + pname);
        System.out.println("Enter price: " + price);
    }
	public class Compare {
	void check(Product obj1,Product obj1,Product obj1)
	{
	if(obj1.price<obj2.price&&obj1.price<obj3.price)
	{
	System.out.print(obj1.pname+"has lowest price");
	}
	else if(obj2.price<obj2.price&&obj2.price<obj3.price)
	{
	System.out.print(obj2.pname+"has lowest price");
	}
	else
	{
	System.out.print("same price occured");
	}

    public static void main(String args[]) {
        Product obj1 = new Product();
	Product obj2 = new Product();
	Product obj3 = new Product();
        obj1.input();
        obj1.display();
	obj2.input();
        obj2.display();
	obj3.input();
        obj3.display();
    	Compare cmp=new Compare();
	cpm.compare();
    }
}
